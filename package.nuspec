      </group>
      <group targetFramework="Xamarin.iOS10">
        <dependency id="Xamarin.Firebase.iOS.AdMob" version="7.31.0.2"/>
        <dependency id="Xamarin.Google.iOS.MobileAds" version="7.31.0.2"/>
      </group>
      <group targetFramework="uap">
        <!--<dependeciy id="iTextSharp" version="5.5.9" />-->
        <!--<dependency id="MSBuild.Sdk.Extras" version="1.4.0"/>-->
        <dependency id="Microsoft.NetCore.UniversalWindowsPlatform" version="6.0.6"/>
      </group>
    </dependencies>
    <contentFiles>
