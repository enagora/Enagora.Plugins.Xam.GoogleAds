<?xml version="1.0"?>
<package>
  <metadata minClientVersion="2.8.1">
    <id>Enagora.Plugins.Xam.GoogleAds</id>
    <version>0.0.5-beta</version>
    <title>Xamarin Google Ads Plugin</title>
    <authors>Uraitz Olaizola</authors>
    <owners>Enagora</owners>
    <licenseUrl>https://github.com/enagora/Enagora.Plugins.Xam.GoogleAds/blob/master/LICENSE</licenseUrl>
